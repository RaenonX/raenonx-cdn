{
  "name": "raenonx-cdn",
  "version": "1.0.0",
  "description": "A microservice CDN using Sharp (for image) and Fastify",
  "main": "dist/src/index.js",
  "scripts": {
    "build": "tsc && tsc-alias",
    "lint": "eslint --config eslint.config.mjs --cache src",
    "lint:fix": "eslint --config eslint.config.mjs --fix src",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/src/index.js",
    "clean": "rimraf dist",
    "tsc": "tsc --noEmit",
    "prebuild": "yarn clean"
  },
  "author": "RaenonX",
  "license": "ISC",
  "dependencies": {
    "@fastify/cors": "^8.4.0",
    "@fastify/static": "^6.12.0",
    "@isaacs/ttlcache": "^1.4.1",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "envalid": "^8.1.0",
    "fastify": "^4.24.3",
    "js-yaml": "^4.1.0",
    "sharp": "^0.32.6"
  },
  "devDependencies": {
    "@eslint/compat": "^1.3.2",
    "@jest/globals": "^30.1.2",
    "@stylistic/eslint-plugin": "^5.3.1",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.8.10",
    "@typescript-eslint/parser": "^8.42.0",
    "eslint": "^9.34.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-unused-imports": "^4.2.0",
    "globals": "^16.3.0",
    "jest": "^30.1.3",
    "pino-pretty": "^13.1.1",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.20.5",
    "typescript": "^5.2.2",
    "typescript-eslint": "^8.42.0"
  },
  "engines": {
    "node": ">=24.0"
  },
  "packageManager": "yarn@4.9.4"
}
